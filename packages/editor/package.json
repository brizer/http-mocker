{
  "name": "http-mockjs-ui",
  "version": "4.4.0",
  "description": "GUI for http-mockjs",
  "keywords": [
    "GUI",
    "http-mockjs",
    "mock",
    "mockjs"
  ],
  "author": "brizer <brizer@163.com>",
  "homepage": "https://github.com/brizer/http-mocker#readme",
  "license": "MIT",
  "main": "lib/editor.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "bin": {
    "http-mockjs-ui": "./dist/server/index.js"
  },
  "files": [
    "/dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brizer/http-mocker.git"
  },
  "scripts": {
    "dev:ui": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "dev:server": "cross-env NODE_ENV=development nodemon",
    "build": "rm -rf dist && npm run build:editor:ui && npm run build:editor:server",
    "build:editor:ui": "cd ../editor && cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "build:editor:server": "cd server && cross-env NODE_ENV=production tsc --project ../tsconfig.server.json"
  },
  "bugs": {
    "url": "https://github.com/brizer/http-mocker/issues"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chokidar": "^3.0.1",
    "express": "^4.17.1",
    "http-mockjs-util": "^4.3.0",
    "i18next": "^13.1.5",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "jsonfile": "^5.0.0",
    "minimist": "^1.2.0",
    "portfinder": "^1.0.20",
    "react-i18next": "^9.0.6",
    "read-pkg": "^5.1.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/express": "^4.16.1",
    "@types/jsonfile": "^5.0.0",
    "@types/minimist": "^1.2.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.15",
    "antd": "^3.19.2",
    "babel-plugin-import": "^1.12.0",
    "cross-env": "^5.2.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.1",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "serialize-javascript": "^2.1.2",
    "styled-components": "^4.2.1",
    "styled-normalize": "^8.0.6",
    "ts-node": "^8.5.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "engines": {
    "node": ">=8.9.1"
  }
}
