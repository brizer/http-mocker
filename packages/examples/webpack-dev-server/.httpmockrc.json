{
    "mockFileName": "mocks",
    "responseHeaders": {
        "Content-Type": "application/json",
        "X-Proxy-By": "http-mockjs"
    },
    "$schema": "http://json.schemastore.org/httpmockrc",
    "routes": {
        "GET /j/getSomeData.json": {
            "path": "/api/get.json",
            "ignore": false
        },
        "POST /p/postData.do": {
            "path": "/api/post.json",
            "ignore": false
        },
        "GET /user/:id": {
            "path": "/api/user.json",
            "ignore": false,
            "delay": 300
        },
        "GET /users/:id+": {
            "path": "/api/info.json",
            "ignore": true
        },
        "GET /j/new": {
            "path": "/api/info.json",
            "ignore": false,
            "delay": 1000
        },
        "GET /j/new?id=123": {
            "path": "/api/123.json",
            "ignore": false
        },
        "GET 1577943669014": {
            "path": "/api/test.json",
            "ignore": false
        }
    }
}
